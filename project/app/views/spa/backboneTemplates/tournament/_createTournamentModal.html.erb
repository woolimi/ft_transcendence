<!-- search user modal -->
<div class="modal" id="createTournamentModal" tabindex="-1" role="dialog" aria-labelledby="channelPasswordModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title">Create tournament</h5>
				<button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">
				<form id="create-tournament-form">
					<div class="form-group">
						<label for="name">Tournament name</label>
						<input id=tournament-name type="text" class="form-control" id="name" placeholder="type the tournament name" name="name">
					</div>
					<button type="submit" class="btn btn-primary">Create tournament</button>
				</form>
			</div>
		</div>
	</div>
</div>
	